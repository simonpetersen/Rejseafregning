<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.nothPanel {
		background-color: #BDEDFF;
	}
	.westPanel {
		background-color: #E5E4E2;
	}
	.centerPanel {
		background-color: #FEFCFF;
		padding: 5px;
		margin: 5px;
	}
	</ui:style>
		<g:DockPanel borderWidth="0" width="100%" height="100%" horizontalAlignment="ALIGN_LEFT">
			<g:Dock direction="NORTH">
				<g:HorizontalPanel styleName="{style.nothPanel}" width="100%" ui:field="hPanel">
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:VerticalPanel width="40%" styleName="{style.nothPanel}" ui:field="vPanel1" horizontalAlignment="ALIGN_LEFT" >
						<g:Label text="Navn:" ui:field="navn" width="100%"/>
						<g:Label text="Afdeling:" ui:field="afdeling" width="100%"/>
					</g:VerticalPanel>
					</g:Cell><g:Cell horizontalAlignment="ALIGN_RIGHT">
					
						<g:Button ui:field="btnLogud" text="Log ud" ></g:Button>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="WEST">
				<g:VerticalPanel width="30%" styleName="{style.westPanel}" ui:field="vPanel2">
					<g:Label ui:field="velkommen" text="Velkommen"/>
					<g:Label text="Mine opgaver" ui:field="mineopgaver"/>
					<g:Label text="Dokumentarkiv" ui:field="dokumentarkiv"/>
					<g:Label text="Rapporter" ui:field="rapporter"/>
					<g:Label text="Mine oplysninger" ui:field="mineoplysninger"/>
					<g:Label text="Indstillinger" ui:field="indstillinger"/>
					<g:Label text="Intranet link" ui:field="intralink"/>
					<g:Label text="HjÃ¦lp" ui:field="hjelp"/>
				</g:VerticalPanel>
			</g:Dock>
			<g:Dock direction="CENTER" width="">
				<g:VerticalPanel horizontalAlignment="ALIGN_LEFT" width="70%" height="80%" styleName="{style.centerPanel}" ui:field="vPanel3">
					<g:Label text="Test"/>
				</g:VerticalPanel>
			</g:Dock>
		</g:DockPanel>
		
</ui:UiBinder> 
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.overskrift {
		padding: 5px;
		background-color: #BDEDFF;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%">
			<g:Grid width="100%">
				<g:row>
					<g:customCell>
						<g:Label text="Rejsedage" styleName="{style.overskrift}"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:VerticalPanel ui:field="rejsedagePanel" width="100%"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Button text="Ny rejsedag" ui:field="nyRejsedag"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Udgifter:" styleName="{style.overskrift}"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:VerticalPanel ui:field="udgifterPanel" width="100%"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Button text="Ny udgift" ui:field="nyUdgift"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Bilag:" styleName="{style.overskrift}"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Button text="TilfÃ¸j" ui:field="tilfoej"/>
					</g:customCell>
				</g:row>
			</g:Grid>
			<g:Button text="Gem og Afslut" ui:field="gemogafslut"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
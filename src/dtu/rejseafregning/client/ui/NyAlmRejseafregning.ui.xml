<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.overskrift {
		padding: 5px;
		background-color: #BDEDFF;
	}
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:ScrollPanel height="100%" width="100%" ui:field="scPanel1">
			<g:VerticalPanel ui:field="vPanel" width="100%" height="100%">
			<g:Label text="Basis information: " styleName="{style.overskrift}"/>
			<g:HorizontalPanel ui:field="hPanel1">
				<g:Label text="Land: " ui:field="land"/>
				<g:ListBox ui:field="dropDownLand"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel ui:field="hPanel2">
				<g:Label text="By: " ui:field="by"/>
				<g:ListBox ui:field="dropDownBy"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel ui:field="hPanel3">
				<g:Label text="Rejseformål: " ui:field="rejseformål"/>
				<g:ListBox ui:field="dropDownRejseformål"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel ui:field="hPanel4">
				<g:Label text="Start dato: " ui:field="startDato"/>
				<p1:DatePicker ui:field="datePicker1"/>
				<g:Label text="Slut dato: " ui:field="slutDato"/>
				<p1:DatePicker ui:field="datePicker2"/>
			</g:HorizontalPanel>
			<g:Label text="Anledning: " ui:field="anledning"/>
			<g:TextBox ui:field="andledText"/>
			<g:Label text="Forklaring" ui:field="forklaring"/>
			<g:TextBox ui:field="forklaringText"/>
			<g:Label text="Dimentioner: " ui:field="dimentioner" styleName="{style.overskrift}"/>
			<g:HorizontalPanel ui:field="hPanel14">
				<g:VerticalPanel ui:field="vPanel2">
				<g:HorizontalPanel ui:field="hPanel5">
					<g:Label text="Projekt:" ui:field="projekt"/>
				<g:ListBox ui:field="dropDownPro"/>
				<g:Button text="Søg" ui:field="seach1"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel6">
				<g:Label text="Opgave: " ui:field="opgave"/>
				<g:ListBox ui:field="dropDownOpg1"/>
				<g:Button ui:field="search2">Søg</g:Button>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel7">
				<g:Label text="Udgiftsorganisation: " ui:field="udgiftsOrg"/>
				<g:ListBox ui:field="dropDownUdg"/>
				<g:Button text="Søg" ui:field="seach3"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel8">
				<g:Label text="Underkonto: " ui:field="underkonto"/>
				<g:ListBox ui:field="dropDownUnd"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel9">
				<g:Label text="Sted: " ui:field="sted"/>
				<g:ListBox ui:field="dropDownSted"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel10">
				<g:Label text="Analyse: " ui:field="analyse"/>
				<g:ListBox ui:field="dropDownAnalyse"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel11">
				<g:Label text="Momskode" ui:field="moms"/>
				<g:ListBox ui:field="dropDownMoms"/>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel12">
				<g:Label text="Periode" ui:field="periode"/>
				<g:ListBox ui:field="dropDownPer"/>
				<g:Button ui:field="seach4">Søg</g:Button>
			</g:HorizontalPanel>
				<g:HorizontalPanel ui:field="hPanel13">
				<g:Label text="Opdeling %" ui:field="opdeling"/>
				<g:IntegerBox ui:field="opdelingInt1"/>
			</g:HorizontalPanel>
			</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel3">
					<g:Label text="Søgekriterier: " ui:field="searchkriterier"/>
					<g:Label text="Navn: " ui:field="name"/>
					<g:TextBox ui:field="name2"/>
					<g:Label text="Nummer: " ui:field="number"/>
					<g:TextBox ui:field="number2"/>
					<g:Label text="Nummer og Navn: " ui:field="numberandname"/>
					<g:ListBox visibleItemCount="5" ui:field="numberandname2"/>
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:Label text="Opdeling af kontostreng: " ui:field="opdKonto1" styleName="{style.overskrift}"/>
			<g:HorizontalPanel ui:field="hPanel16">
				<g:VerticalPanel ui:field="vPanel17">
					<g:Label text="Projekt: " ui:field="projekt2"/>
					<g:Label text="New Label" ui:field="projekt3"/>
				</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel18">
					<g:Label text="Opgave: " ui:field="opgave4"/>
					<g:Label text="New Label" ui:field="opgave5"/>
				</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel19">
					<g:Label text="Udgiftsorganisation: " ui:field="udgOrg2"/>
					<g:Label text="New Label" ui:field="udgOrg3"/>
				</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel20">
				<g:Label text="Sted: " ui:field="sted2"/>
				<g:Label text="New Label" ui:field="sted3"/>
			</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel21">
					<g:Label text="Opdeling %: " ui:field="nyopdkonto4"/>
					<g:Label text="New Label" ui:field="opdelingPro2"/>
				</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel22">
					<g:Label text="Opdeling kr.: " ui:field="nyopdkonto5"/>
					<g:Label text="New Label" ui:field="opgaveDoub3"/>
				</g:VerticalPanel>
				<g:Button text="Slet" ui:field="slet"/>
			</g:HorizontalPanel>
			<g:Button text="Ny opdelign af kontostreng" ui:field="nyopdkonto"/>
			<g:VerticalPanel ui:field="vPanel6">
				<g:HorizontalPanel ui:field="hPanel15">
				<g:VerticalPanel ui:field="vPanel4">
				<g:Label text="Opdeling i procenter (%) : " ui:field="nyopdkonto2"/>
				<g:Label text="Opgave: " ui:field="opgave2"/>
				<g:ListBox ui:field="dropDownOpg2"/>
				<g:Label text="Opdeling %" direction="LTR" ui:field="opdelingPro"/>
				<g:IntegerBox ui:field="opgaveInt2"/>
			</g:VerticalPanel>
				<g:VerticalPanel ui:field="vPanel5">
					<g:Label text="Opdeling i kroner (kr.):" direction="LTR" ui:field="nyopdkonto3"/>
					<g:Label text="Opgave" ui:field="opgave3"/>
					<g:ListBox ui:field="dropDownOpg3"/>
					<g:Label text="Opdeling kr." ui:field="opdelingKr"/>
					<g:DoubleBox ui:field="opgaveDoub2"/>
				</g:VerticalPanel>
			</g:HorizontalPanel>
				<g:Button text="Tilføj" ui:field="addOpdeling"/>
			</g:VerticalPanel>
			<g:Button text="Gem og næste" ui:field="gemogneste"/>
		</g:VerticalPanel>
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">
	<ui:style>
	.oplysningerGrid {
		border: 1px solid #666666;
	}
	</ui:style>
	<g:VerticalPanel styleName="h2">
		<g:Label text="Mine oplysninger" styleName="h2"/>
		<g:Grid styleName="{style.oplysningerGrid}">
			<g:row>
				<g:customCell>
					<g:Label text="Navn: "/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="navnTextBox"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Postnummer: "/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="postnrTextBox"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Brugernavn: "/>
				</g:customCell>
				<g:customCell>
					<g:Label ui:field="brugerNavnLabel" />
				</g:customCell>
				<g:customCell>
					<g:Label text="By: "/>
				</g:customCell>
				<g:customCell>
					<g:Label text="" ui:field="byLabel"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Email: "/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="emailTextBox"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Vej: "/>
				</g:customCell>
				<g:customCell>
					<g:SuggestBox ui:field="vejTextBox"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Ny adgangskode:  "/>
				</g:customCell>
				<g:customCell>
					<g:PasswordTextBox ui:field="adgangskodeTextBox"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Gentag adgangskode:  "/>
				</g:customCell>
				<g:customCell>
					<g:PasswordTextBox ui:field="adgangskode2TextBox"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Button text="Opdater" ui:field="gemKnap" />
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:VerticalPanel>
</ui:UiBinder> 